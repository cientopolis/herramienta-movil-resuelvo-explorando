        <EducationalActivity>
			<Name>¡Aprendo Jugando!</Name>
			<Goal>El objetivo del juego es recolectar elementos de acuerdo a una tarea recibida, para luego depositarlos según el tipo de material con el que están hechos.</Goal>
			<Code>E1</Code>
			<Tasks>
				<Task>task1</Task>
				<Task>task2</Task>
				<Task>task3</Task>
			</Tasks>
		</EducationalActivity>
    </EducationalActivities>
	
	<Tasks>
		<Task>
			<Name>task1</Name>
			<Description>Deberán recolectar todos los elementos que se puedan usar para hacer papel reciclado.</Description>
			<Code>task1</Code>
			<ValidElements>
				<Element>cajaDeCereal</Element>
				<Element>hojaDeCuaderno</Element>
				<Element>tuboDeRolloDePapel</Element>
				<Element>cajaParaHuevos</Element>
			</ValidElements>
			<Elements>
				<Element>cajaDeCereal</Element>
				<Element>hojaDeCuaderno</Element>
				<Element>sachetDeLeche</Element>
				<Element>tuboDeRolloDePapel</Element>
				<Element>envaseDeYogurth</Element>
				<Element>envaseDeGaseosa</Element>
				<Element>envaseDeGaseosa</Element>
				<Element>perchaGrande</Element>
				<Element>vasoGrande</Element>
				<Element>vasoChico</Element>
				<Element>cajaParaHuevos</Element>
			</Elements>
		</Task>
		
		<Task>
			<Name>task2</Name>
			<Description>Deberán recolectar todos los elementos que se puedan usar para envasar líquidos.</Description>
			<Code>task2</Code>
			<ValidElements>
				<Element>botellaDeGaseosa</Element>
				<Element>botellaDeLeche</Element>
				<Element>envaseDeGaseosa</Element>
				<Element>botellaChica</Element>
			</ValidElements>
			<Elements>
				<Element>botellaDeGaseosa</Element>
				<Element>botellaDeLeche</Element>
				<Element>cajaDeZapatos</Element>
				<Element>platoDescartable</Element>
				<Element>coladorDePasta</Element>
				<Element>envaseDeGaseosa</Element>
				<Element>botellaChica</Element>
				<Element>rolloGrande</Element>
				<Element>rolloChico</Element>
				<Element>sobreGrande</Element>
			</Elements>
		</Task>
		
		<Task>
			<Name>task3</Name>
			<Description>Deberán recolectar todos los elementos que se puedan usar para medir distancia en centímetros.</Description>
			<Code>task3</Code>
			<ValidElements>
				<Element>reglaGrande</Element>
				<Element>escuadra</Element>
			</ValidElements>
			<Elements>
				<Element>reglaGrande</Element>
